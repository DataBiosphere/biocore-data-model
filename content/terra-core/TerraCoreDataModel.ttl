<https://datamodel.terra.bio/TerraCore>
  a owl:Ontology ;
  dc:contributor "Sid Cox" ;
  dc:creator "Kathy Reinold" ;
  dct:date "20 Jul 2020" ;
  dct:license <https://github.com/DataBiosphere/terra-interoperability-model/blob/master/LICENSE> ;
