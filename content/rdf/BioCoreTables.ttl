<https://datamodel.terra.bio/BioCore>
  a owl:Ontology ;
  owl:versionInfo "February 2023 Sid Cox" ;
.
BioCore:Activity
  a owl:Class ;
  rdfs:label "Activity" ;
  rdfs:subClassOf prov:Activity ;
  skos:prefLabel "Activity" ;
  prov:definition "An activity occurs over a period of time, generates an entity, and may include consuming, processing, transforming, modifying, relocating, or using entities.  This subclass of prov:Activity restricts members to those with a DataModality relevant to biomedical research." ;
.
BioCore:Sample
  a owl:Class ;
  rdfs:label "Sample" ;
  skos:definition "Data about a physical material collected for the purpose of research." ;
  skos:prefLabel "Sample" ;
.
BioCore:Donor
  a owl:Class ;
  rdfs:label "Donor" ;
  rdfs:subClassOf obo:OBI_0100026 ; # donor is a subclass of organism
  skos:definition "An organism from which a sample or test result is available" ;
  skos:prefLabel "Donor" ;
.
BioCore:Antibody
  a owl:Class ;
  rdfs:label "Antibody" ;
  rdfs:subClassOf obo:BFO_0000040 ;
  prov:definition "A reference to the antibody/antibodies used in this Activity." ;
.
BioCore:AlignmentActivity
  a owl:Class ;
  rdfs:label "Alignment" ;
  rdfs:subClassOf BioCore:Activity ;
  skos:prefLabel "Alignment" ;
  prov:definition "An activity that aligns the output of a Sequencing Activity (a genetic sequence) to produce an aligned sequence." ;
.
TerraCore:AlignmentFile
  a owl:Class ;
  rdfs:label "AlignmentFile" ;
  rdfs:subClassOf TerraCore:File ;
  skos:definition "An electronic record of the alignment of a genetic sequence for a particular Sample compared with a reference sequence generated by an AlignmentActivity." ;
  skos:prefLabel "AlignmentFile" ;
.
BioCore:AnalysisActivity
  a owl:Class ;
  rdfs:label "Analysis" ;
  rdfs:subClassOf BioCore:Activity ;
  skos:prefLabel "Analysis" ;
  prov:definition "An activity that records the output of a specific analysis method.  Software version, components, arguments are described in prov:SoftwareAgent." ;
.
BioCore:AssayActivity
  a owl:Class ;
  rdfs:label "Assay" ;
  rdfs:subClassOf BioCore:Activity ;
  skos:prefLabel "Assay" ;
  prov:definition "An activity that generates an electronic record documenting the result of the Assay performed." ;
.
BioCore:BioSample
  a owl:Class ;
  rdfs:label "BioSample" ;
  rdfs:subClassOf BioCore:Sample ;
  skos:altLabel "Biospecimen" ;
  skos:altLabel "Sample" ;
  skos:altLabel "Specimen from organism" ;
  skos:definition "Data about a physical sample consisting of one or more cells taken from an organism (living or deceased) or derived from such a Sample." ;
  skos:prefLabel "BioSample" ;
.
BioCore:BioSampleDissociationActivity
  a owl:Class ;
  rdfs:label "BioSampleDissociation" ;
  rdfs:subClassOf BioCore:Activity ;
  owl:equivalentClass <http://www.ebi.ac.uk/efo/EFO_0009091> ;
  skos:definition "Activity which results in the separation of a BioSample into individual cells or a cell suspension." ;
.
BioCore:Diagnosis
  a owl:Class ;
  rdfs:label "Diagnosis" ;
  rdfs:subClassOf obo:IAO_0000030 ;
  skos:definition "Result of a medical investigation that often identifies a disease or condition." ;
.
BioCore:DogDonor
  a owl:Class ;
  rdfs:label "DogDonor" ;
  rdfs:subClassOf BioCore:Donor ;
  skos:definition "Extension of Donor class for dogs." ;
.
BioCore:Donor
  a owl:Class ;
  rdfs:label "Donor" ;
  rdfs:subClassOf obo:OBI_0100026 ;
  skos:definition "An organism from which a sample or test result is available" ;
  skos:prefLabel "Donor" ;
.
BioCore:DonorTreatmentActivity
  a owl:Class ;
  rdfs:label "Donor Treatment" ;
  rdfs:subClassOf BioCore:Activity ;
  skos:definition "Activities or processes undertaken with the expectation of therapeutic value to the Donor." ;
  skos:prefLabel "Donor Treatment" ;
.
BioCore:ExperimentActivity
  a owl:Class ;
  rdfs:label "Experiment" ;
  rdfs:subClassOf BioCore:Activity ;
  skos:definition " An operation or procedure carried out under controlled conditions to make a discovery or to test or establish a hypothesis. In the context of this data model, an experiment is a collection of related activities, possibly including sample preparation, sequencing, assays and/or downstream analysis. " ;
  skos:prefLabel "Experiment" ;
.
BioCore:FamilyMember
  a owl:Class ;
  rdfs:label "Family Member" ;
  rdfs:subClassOf obo:OBI_0100026 ;
  skos:definition "Family member has biological relationship to a Donor.  Limited health information.  A family member for whom samples, imaging studies, lab tests or other healthcare information is available may be considered a Donor." ;
.
BioCore:File
  a owl:Class ;
  rdfs:comment "Used definition from http://semanticscience.org/resource/SIO_000396 but equivalence to http://www.w3.org/ns/dcat#Distribution doesn't make sense in this context; thus we have our own class.  Also added electronic. " ;
  rdfs:label "File" ;
  rdfs:subClassOf obo:IAO_0000030 ;
  skos:definition "A file is an information-bearing electronic object that contains a physical embodiment of some information using a particular character encoding." ;
  skos:prefLabel "File" ;
.
BioCore:HistopathologyActivity
  a owl:Class ;
  rdfs:label "Histopathology" ;
  rdfs:subClassOf BioCore:Activity ;
.
BioCore:HumanDonor
  a owl:Class ;
  rdfs:label "HumanDonor" ;
  rdfs:subClassOf BioCore:Donor ;
  skos:definition "Extension of Donor class for Humans which includes attributes specific to human donors." ;
.
BioCore:Library
  a owl:Class ;
  rdfs:label "Library" ;
  rdfs:subClassOf obo:BFO_0000040 ;
  skos:definition "A collection of nucleic acid molecules." ;
  skos:prefLabel "Library" ;
.
BioCore:LibraryPreparationActivity
  a owl:Class ;
  rdfs:label "LibraryPreparation" ;
  rdfs:subClassOf BioCore:Activity ;
  skos:definition "Activity which results in the creation of a library from fragments of genomic material in preparation for further analysis." ;
  skos:prefLabel "LibraryPreparation" ;
.
BioCore:Measurement
  a owl:Class ;
  rdfs:label "Measurement" ;
  rdfs:subClassOf obo:IAO_0000030 ;
  skos:definition "The record of the size, quantity or outcome of a clinical or laboratory test." ;
  skos:prefLabel "Measurement" ;
.
BioCore:MolecularSample
  a owl:Class ;
  rdfs:label "MolecularSample" ;
  rdfs:subClassOf BioCore:Sample ;
  skos:prefLabel "MolecularSample" ;
  prov:definition "Data about a physical sample consisting of one or more molecular entities, either purified from a Sample or synthesized." ;
.
BioCore:MouseDonor
  a owl:Class ;
  rdfs:label "MouseDonor" ;
  rdfs:subClassOf BioCore:Donor ;
  skos:definition "Extension of Donor class for mice." ;
.
BioCore:NucleusIsolationActivity
  a owl:Class ;
  rdfs:label "NucleusIsolation" ;
  rdfs:subClassOf BioCore:Activity ;
  owl:equivalentClass <http://www.ebi.ac.uk/efo/EFO_0007831> ;
  skos:definition "Activity to separate the nucleus or nuclei from the cell.  " ;
.
BioCore:Project
  a owl:Class ;
  rdfs:label "Project" ;
  rdfs:subClassOf prov:Activity ;
  skos:definition "A collective effort with an objective related to biomedical research." ;
  skos:prefLabel "Project" ;
  .
BioCore:ReferenceAssembly
  a owl:Class ;
  obo:IAO_0000114 "DRAFT" ;
  rdfs:comment "Instances should include either a cross reference to the reference sequence or an official GRCName from the Genome Reference Consortium, or both." ;
  rdfs:label "ReferenceAssembly" ;
  rdfs:subClassOf obo:OBI_0001573 ;
  skos:prefLabel "ReferenceAssembly" ;
  prov:definition "DNA sequence data identified as representative of specific organism's DNA." ;
.
BioCore:Sample
  a owl:Class ;
  rdfs:label "Sample" ;
  rdfs:subClassOf obo:OBI_0100051 ;
  skos:definition "Data about a physical material collected for the purpose of research." ;
  skos:prefLabel "Sample" ;
.
TerraCore:SampleTreatmentActivity
  a owl:Class ;
  rdfs:label "SampleTreatmentActivity" ;
  rdfs:subClassOf TerraCore:Activity ;
  skos:definition "Activity which transforms a BioSample into another BioSample. Typically the sample treatment activity involves adding something to cells or tissue, for example, treatment with chemicals, drugs, biological infection, irradiation, exposures, or other non-genetic modifications. This treatment excludes mechanical separation of a BioSample into single cells (covered by BioSampleDissociation), and is not needed for genetic modifications to cells (covered by hasGeneticModification)." ;
.
BioCore:SequenceFile
  a owl:Class ;
  rdfs:label "SequenceFile" ;
  rdfs:subClassOf BioCore:File ;
  skos:definition "An electronic record of the genetic sequence for a particular Sample generated by a SequencingActivity." ;
  skos:prefLabel "SequenceFile" ;
.
BioCore:SequenceLocation
  a owl:Class ;
  rdfs:label "SequenceLocation" ;
  rdfs:subClassOf obo:OBI_0000973 ;
  skos:prefLabel "SequenceLocation" ;
  prov:definition "The location of a sequence feature defined by its start and end position based on a reference coordinate system, such as recorded in a sequence feature table." ;
.
BioCore:SequencingActivity
  a owl:Class ;
  rdfs:label "Sequencing" ;
  rdfs:subClassOf BioCore:Activity ;
  skos:prefLabel "Sequencing" ;
  prov:definition "An activity that produces an electronic record of a genetic sequence from a Sample" ;
.
BioCore:SingleCellAssignment
  a owl:Class ;
  rdfs:label "SingleCellAssignment" ;
  rdfs:subClassOf BioCore:Activity ;
  skos:definition "An activity that infers a cell type based on analysis.  " ;
.
BioCore:SingleCellIsolationActivity
  a owl:Class ;
  rdfs:label "SingleCellIsolation" ;
  rdfs:subClassOf BioCore:Activity ;
  skos:definition "Activity to separate individual cells or nuclei. " ;
.
BioCore:VariantCall
  a owl:Class ;
  rdfs:label "VariantCall" ;
  rdfs:subClassOf obo:OBI_0000973 ;
  skos:prefLabel "VariantCall" ;
  prov:definition "A variation in genetic sequence for a particular Sample and VariantCallingActivity." ;
.
BioCore:VariantCallSetFile
  a owl:Class ;
  rdfs:label "VariantCallSet" ;
  rdfs:subClassOf BioCore:File ;
  skos:prefLabel "VariantCallSet" ;
  prov:definition "An electronic record of a collection of variations in genetic sequence for a particular Sample that have been generated by a VariantCallingActivity." ;
.
BioCore:VariantCallingActivity
  a owl:Class ;
  rdfs:label "VariantCallingActivity" ;
  rdfs:subClassOf BioCore:Activity ;
  skos:definition "An activity that identifies genetic sequence alterations in a Sample when compared to a reference." ;
  skos:prefLabel "VariantCalling" ;
.